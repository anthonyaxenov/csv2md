    "version": "0.2.0",
    "configurations": [
        {
            "name": "csv",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "-f=example.csv",
            ]
        },
        {
            "name": "csv aligned",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "-f=example.csv",
                "-a"
            ]
        },
        {
            "name": "csv header",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "-f=example.csv",
                "-h=\"My Header\"",
            ]
        },
        {
            "name": "tsv",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "-f=example.tsv",
                "-t"
            ]
        },
        {
            "name": "tsv aligned",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "-f=example.tsv",
                "-t",
                "-a"
            ]
        },
        {
            "name": "tsv header",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "main.go",
            "args": [
                "-f=example.tsv",
                "-h=\"My Header\"",
                "-t"
            ]
        },
    ]
}
